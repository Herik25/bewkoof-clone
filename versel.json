{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "build/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/products",
            "dest": "index.html"
        },
        {
            "src": "/users",
            "dest": "index.html"
        },
        {
            "src": "/auth",
            "dest": "index.html"
        },
        {
            "src": "/cart",
            "dest": "index.html"
        },
        {
            "src": "/orders",
            "dest": "index.html"
        },
        {
            "src": "/products/(.*)",
            "dest": "index.html"
        },
        {
            "src": "/users/(.*)",
            "dest": "index.html"
        },
        {
            "src": "/auth/(.*)",
            "dest": "index.html"
        },
        {
            "src": "/cart/(.*)",
            "dest": "index.html"
        },
        {
            "src": "/orders/(.*)",
            "dest": "index.html"
        },
        {
            "src": "/create-payment-intent",
            "dest": "index.html"
        },
        {
            "src": "/webhook",
            "dest": "index.html"
        },
        {
            "src": "/admin",
            "dest": "build/index.html"
        },
        {
            "src": "/admin/(.*)",
            "dest": "build/index.html"
        },
        {
            "src": "/productdetail",
            "dest": "build/index.html"
        },
        {
            "src": "/productdetail/(.*)",
            "dest": "build/index.html"
        },
        {
            "src": "/all-products",
            "dest": "build/index.html"
        },
        {
            "src": "/all-products/(.*)",
            "dest": "build/index.html"
        },
        {
            "src": "/login",
            "dest": "build/index.html"
        },
        {
            "src": "/login/(.*)",
            "dest": "build/index.html"
        },
        {
            "src": "/signup",
            "dest": "build/index.html"
        },
        {
            "src": "/signup/(.*)",
            "dest": "build/index.html"
        },
        {
            "src": "/user-cart",
            "dest": "build/index.html"
        },
        {
            "src": "/user-cart(.*)",
            "dest": "build/index.html"
        },
        {
            "src": "/checkout",
            "dest": "build/index.html"
        },
        {
            "src": "/checkout/(.*)",
            "dest": "build/index.html"
        },
        {
            "src": "/myaddresses",
            "dest": "build/index.html"
        },
        {
            "src": "/ordersuccess/(.*)",
            "dest": "build/index.html"
        },
        {
            "src": "/ordersuccess",
            "dest": "build/index.html"
        },
        {
            "src": "/myorders",
            "dest": "build/index.html"
        },
        {
            "src": "/myaccount",
            "dest": "build/index.html"
        },
        {
            "src": "/myprofile",
            "dest": "build/index.html"
        },
        {
            "src": "/men-products",
            "dest": "build/index.html"
        },
        {
            "src": "/women-products",
            "dest": "build/index.html"
        },
        {
            "src": "/aboutus",
            "dest": "build/index.html"
        },
        {
            "src": "/privacy-policy",
            "dest": "build/index.html"
        },
        {
            "src": "/terms",
            "dest": "build/index.html"
        },
        {
            "src": "/",
            "dest": "build/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "build/index.html"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}