{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
          "src": "/((?!.*\\.\\w+$).*)",
          "dest": "/index.js"
        },
        {
          "src": "/(.+\\.[a-z]+)$",
          "dest": "/dist/$1"
        }
      ]
}